[sub_resource type="GDScript" id="GDScript_o6xl0"]
script/source = "extends SubViewportContainer

@onready var player = get_node(\"SubViewport/Player\")

func _ready():
	set_process_unhandled_input(true)

func _input(event):
	if event is InputEventMouse:
		var mouseEvent = event.duplicate()
		mouseEvent.position = get_global_transform_with_canvas().affine_inverse() * event.position
		player._unhandled_input(mouseEvent)
	else:
		player._unhandled_input(event)
"

